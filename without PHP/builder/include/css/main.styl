    left 12px

  .input-block
    width 66%
    margin: 32px 0 0 17%;

  .Rectangle-2
    width: 16px;
    height: 16px;
    border-radius: 4px;
    border: solid 1px rgba(0, 0, 0, 0.2);

  .one-checkbox
    width: 100%;
    margin 32px 0 0 0
  label.label-checkbox
