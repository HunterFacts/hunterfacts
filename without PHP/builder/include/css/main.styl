  .two-checkbox
    width: 100%;
    margin 25px 0 0 0
  .Rectangle-3-Copy-5
    width 100%
    height 60px
    font-size 20px
    text-align center

  label.or-sign-up-with-serv
    width 66%
    height 20px
    font-size 16px
    margin: 32px 0 0 17%;

  .four-labels
    width 66%
    margin 20px 17% 25px 17%
    display: flex;
    justify-content space-around

  .four-labels a
    width 35px
    height 36px
    display block
    float none

  .label
    width 35px
    height 36px
    margin 0 0 0 auto
    display: flex;

  .four-labels a:first-child .label
    margin 0

