@font-face {font-family: "Gerbera";
  src: url("fonts/Gerbera.eot");
  src: url("fonts/Gerbera.eot?#iefix") format("embedded-opentype"),
  url("fonts/Gerbera.woff2") format("woff2"),
  url("fonts/Gerbera.woff") format("woff"),
  url("fonts/Gerbera.ttf") format("truetype"),
  url("fonts/Gerbera.svg#Gerbera") format("svg");
}
@font-face {font-family: "Lato";
  src: url("fonts/Lato.eot");
  src: url("fonts/Lato.eot?#iefix") format("embedded-opentype"),
  url("fonts/Lato.woff2") format("woff2"),
  url("fonts/Lato.woff") format("woff"),
  url("fonts/Lato.ttf") format("truetype"),
  url("fonts/Lato.svg#Lato") format("svg");
}
body
  background-color #eef5ff
  margin 0
  padding 0

.shape
  width 520px
  height 700px
  background-color #ffffff
  background url('../img/shape.png')
  transform: translate(-50%, -50%);
  position: absolute;
  top: 50%;
  left: 50%;

.inline
  top -50px
  left 520px
  position absolute
  width 57px
  height 98px
  object-fit contain
  background url('../img/three-line.png')

.logo-board
  background url('../img/logo-realtime-board.png')
  background-size cover
  position absolute
  width 140px
  height 40px
  top 28px
  left 80px

h1.Get-started-for-free-Copy
  height 40px
  font-family Gerbera
  font-size 32px
  font-weight bold
  font-style normal
  font-stretch normal
  line-height 1.25
  letter-spacing normal
  color #000000
  text-align left
  margin 60px 100px 0 100px

span.No-credit-card-requi-Copy {
  width: 164px;
  height: 24px;
  font-family: Lato;
  font-size: 16px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.5;
  letter-spacing: normal;
  color: #000000;
  margin: 4px 0 0 101px;
}
a.BG
  width 108px
  height 48px
  border-radius 4px
  border solid 1px rgba(42, 121, 255, 0.8)
  width 42px
  height 16px
  font-family Lato
  font-size 16px
  font-weight normal
  font-style normal
  font-stretch normal
  line-height 1
  letter-spacing normal
  color #2a79ff
  padding 17px 33px
  position absolute
  right 40px
  top 24px

input.Rectangle-3-Copy-3
  width: 278px;
  height: 48px;
  background-color: #ffffff;
  border: solid 1px #cccccc;
  font-family: Lato;
  font-size: 16px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  letter-spacing: normal;
  color: #999999;
  padding 0 0 0 42px
.check-input .input-text:first-child input.Rectangle-3-Copy-3
  border-radius 4px 4px 0 0
  border-bottom 0
.check-input .input-text:last-child input.Rectangle-3-Copy-3
  border-radius 0 0 4px 4px
  border-top 0

.input-text
  position relative

img.img-input
  display block
  position absolute
  top 15px
  left 12px
.input-block
  margin: 32px 0 0 100px;
.Rectangle-2
  width: 16px;
  height: 16px;
  border-radius: 4px;
  border: solid 1px rgba(0, 0, 0, 0.2);
.one-checkbox
  width: 320px;
  margin-top 35px
label.label-checkbox
  font-family: Lato;
  font-size: 14px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.43;
  letter-spacing: normal;
  color: #999999;
  margin-left 28px
  display block
.checkbox-height
  height 40px
  float left
label.label-checkbox a
  color: #999999;
.two-checkbox
  width: 320px;
  margin-top 12px
.Rectangle-3-Copy-5
  width 320px
  height 60px
  border-radius 4px
  background-color #2a79ff
  font-family Lato
  font-size 20px
  font-weight normal
  font-style normal
  font-stretch normal
  line-height 1
  letter-spacing normal
  text-align center
  color #ffffff
  border 0
  margin-top 32px
  cursor pointer
label.or-sign-up-with-serv
  width 320px
  height 20px
  font-family Lato
  font-size 16px
  font-weight normal
  font-style normal
  font-stretch normal
  line-height 1.25
  letter-spacing normal
  color #999999
  text-align center
  display block
  margin: 0 0 0 101px;
  margin-top 32px
.four-labels
  width 240px
  margin 20px 140px 0 141px
.label
  width 35px
  height 36px
  margin 0 0 0 32px
  float left
.four-labels a:first-child .label
  margin 0
@media screen and (min-width: 450px) and (max-width:812px)
